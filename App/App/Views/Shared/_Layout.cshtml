
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - App</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css"/>
</head>
<body>
    <header>
        <h2>Seja bem vindo</h2>
        <div>
            Nome do Usuario:
            <button class="btn btn-danger">Sair</button>
        </div>
    </header> 
    <div class="nav nav-side">
        <ul class="cMenuList" style="list-style: none;">
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="AgroToxic"
                   asp-action="Index">Agrotóxico</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Product"
                   asp-action="Index">Produtos</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Incentive"
                   asp-action="Index">Incentivos</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Tax"
                   asp-action="Index">Taxas</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Company"
                   asp-action="Index">Empresas</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="Produtores"
                   asp-action="Index">Produtores</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-controller="User"
                   asp-action="Index">Usuários</a>
            </li>
        </ul>
    </div>
    <div class="container-fluid content" >
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="final">
            App Construído para a aps da unip - 5 semestre (criado e programado por Lucas Bernardo da Silva)
            &copy; 2021 - App - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
